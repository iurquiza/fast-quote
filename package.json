{
  "name": "fast-quoute",
  "version": "0.1.0",
  "private": true,
  "description": "Fast Quote App",
  "author": "krummelm, iuurquiz, patrguha",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "/",
  "dependencies": {
    "@aws-amplify/api": "^3.2.18",
    "@aws-amplify/auth": "^3.4.18",
    "@aws-amplify/core": "^3.8.20",
    "@aws-amplify/ui-components": "^1.1.0",
    "@aws-amplify/ui-react": "^1.0.5",
    "@craco/craco": "^6.1.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "amazon-cognito-identity-js": "^4.6.0",
    "aws-amplify": "^3.3.25",
    "clsx": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recharts": "^2.0.9",
    "typescript": "^4.2.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "nice": "npm-run-all lint format style",
    "nice:fix": "npm-run-all lint:fix format:fix style:fix",
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --report-unused-disable-directives --ignore-path .gitignore ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --check --ignore-unknown --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "format:fix": "prettier --write --ignore-unknown --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "style": "stylelint --cache --ignore-path .gitignore './src/**/*.{css}'",
    "style:fix": "stylelint --fix --cache --ignore-path .gitignore './src/**/*.{css}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "pretty-quick --staged"
    ],
    "./src/**/*.{css,json,md}": [
      "pretty-quick --staged"
    ],
    "./src/**/*.{css}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-no-unused-selectors": "^1.0.35",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0"
  }
}
